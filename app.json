{
  "expo": {
    "name": "Yogabar-app",
    "slug": "Yogabar-app",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "icon": "./assets/images/logo.png", 
    "scheme": "yogabarapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aman.yogabarapp"
    },
    
    "android": {
      "package": "com.aman.yogabarapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#FFF9E9"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFF9E9"
        }
      ]
    ],
    
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}